<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Regression" verbose="1" >
    <test name="TC#1: Navigate To Login Page">
        <classes>
            <class name="LoginTest">
                <methods>
                    <include name="verifyNavigationToLoginPage"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="TC#2: Verify Successful Login">
        <classes>
            <class name="LoginTest">
                <methods>
                    <include name="verifySuccessfulLogin"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>
